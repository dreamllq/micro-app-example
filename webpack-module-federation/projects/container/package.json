{
  "name": "alsi-aps",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "download:apps": "node ./node_modules/@alsi/micro-app-webpack-config/download-apps/local.cjs",
    "start": "webpack-cli serve --config ./node_modules/@alsi/micro-app-webpack-config/container/webpack.config.development.cjs",
    "dev": "nodemon",
    "build": "node ./node_modules/@alsi/micro-app-webpack-config/download-apps/develop.cjs && webpack --config ./node_modules/@alsi/micro-app-webpack-config/container/webpack.config.production.cjs --env mode=dev",
    "build:preview": "node ./node_modules/@alsi/micro-app-webpack-config/download-apps/develop.cjs && webpack --config ./node_modules/@alsi/micro-app-webpack-config/container/webpack.config.production.cjs --env mode=preview",
    "build:dev": "node ./node_modules/@alsi/micro-app-webpack-config/download-apps/develop.cjs && webpack --config ./node_modules/@alsi/micro-app-webpack-config/container/webpack.config.production.cjs --env mode=dev",
    "build:test": "node ./node_modules/@alsi/micro-app-webpack-config/download-apps/develop.cjs && webpack --config ./node_modules/@alsi/micro-app-webpack-config/container/webpack.config.production.cjs --env mode=test",
    "build:uat": "node ./node_modules/@alsi/micro-app-webpack-config/download-apps/product.cjs && webpack --config ./node_modules/@alsi/micro-app-webpack-config/container/webpack.config.production.cjs --env mode=uat",
    "build:prod": "node ./node_modules/@alsi/micro-app-webpack-config/download-apps/product.cjs && webpack --config ./node_modules/@alsi/micro-app-webpack-config/container/webpack.config.production.cjs --env mode=production",
    "preview": "http-server dist -p 8080 -g -c-1",
    "prepare": "husky install",
    "dev:web": "npm run dev",
    "trans": "alsi-trans -cn src/locales/zh-cn/index.ts -en src/locales/en/index.ts -ja src/locales/ja/index.ts -o src/locales",
    "lint": "eslint ./src/**/*.vue ./src/**/*.js --fix",
    "preversion": "node ./node_modules/@alsi/micro-app-webpack-config/preversion.js",
    "postversion": "git push && git push --tags"
  },
